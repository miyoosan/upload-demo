{
    "name": "upload",
    "version": "0.0.1",
    "private": false,
    "scripts": {
        "serve": "cross-env NODE_ENV=development vue-cli-service serve",
        "build": "cross-env NODE_ENV=production vue-cli-service build",
        "lint-vue": "vue-cli-service lint",
        "lint": "eslint src --fix --ext .js,.vue",
        "prettier": "prettier --write ./src/**/*.vue ./src/**/*.js ./src/**/*.css ./src/**/*.scss",
        "eslint": "eslint --fix ./src/**/*.vue ./src/**/*.js",
        "inspect": "vue-cli-service inspect > output.js",
        "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s"
    },
    "husky": {
        "hooks": {
            "pre-commit": "npm run lint",
            "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
        }
    },
    "lint-staged": {
        "src/**/*.{js,vue}": [
            "npm run lint"
        ]
    },
    "config": {
        "commitizen": {
            "path": "./node_modules/cz-customizable"
        }
    },
    "author": "miyosan <470375124@qq.com> (https://gitee.com/miyosan)",
    "dependencies": {
        "animate.css": "^3.7.2",
        "core-js": "^3.6.4",
        "element-ui": "^2.13.1",
        "hash-wasm": "^4.8.0",
        "normalize.css": "^8.0.1",
        "nprogress": "^0.2.0",
        "uuid": "^8.2.0",
        "vue": "^2.6.11",
        "vue-lazyload": "^1.3.3",
        "vue-router": "^3.1.6"
    },
    "devDependencies": {
        "@commitlint/config-conventional": "^11.0.0",
        "@vue/cli-plugin-babel": "~4.3.0",
        "@vue/cli-plugin-eslint": "~4.3.0",
        "@vue/cli-plugin-router": "~4.3.0",
        "@vue/cli-plugin-unit-jest": "~4.3.0",
        "@vue/cli-plugin-vuex": "~4.3.0",
        "@vue/cli-service": "~4.3.0",
        "@vue/eslint-config-prettier": "^6.0.0",
        "@vue/test-utils": "1.0.0-beta.31",
        "babel-eslint": "^10.1.0",
        "babel-plugin-component": "^1.1.1",
        "commitizen": "^4.2.2",
        "commitlint": "^11.0.0",
        "compression-webpack-plugin": "^6.0.3",
        "conventional-changelog-cli": "^2.1.0",
        "cross-env": "^7.0.2",
        "cz-customizable": "^6.3.0",
        "eslint": "^6.7.2",
        "eslint-plugin-prettier": "^3.1.1",
        "eslint-plugin-vue": "^6.2.2",
        "husky": "^4.3.0",
        "lint-staged": "^10.4.2",
        "prettier": "^1.19.1",
        "script-ext-html-webpack-plugin": "^2.1.4",
        "stylus": "^0.54.8",
        "stylus-loader": "^3.0.2",
        "vue-template-compiler": "^2.6.11"
    }
}
